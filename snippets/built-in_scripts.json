    "color_3_65                  = input.color(#9c27b0, \"\", inline=\"Level7\")",
    "processLevel(show_3_65, value_3_65, color_3_65)",
    "",
    "show_4_236                  = input.bool(true, \"\", inline=\"Level8\")",
